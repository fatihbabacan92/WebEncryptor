
@{
    ViewData["Title"] = "_UploadFile";
}

<h2>Upload File</h2>

<div>
    <form>
        <input type="radio" name="sort" value="RSA" checked /> RSA <br/>
        <input type="radio" name="sort" value="3DES" /> 3DES <br/>
        <a>
            <div class="filePicker">
                <p>Choose or drag File</p>
            </div>
        </a>
        <br />
        <button class="customButton">Submit</button>
    </form>
</div>